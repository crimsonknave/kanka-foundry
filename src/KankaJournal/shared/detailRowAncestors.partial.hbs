{{~#if ancestors.length ~}}
    <dt>
        {{~#if rawLabel~}}
            {{~rawLabel~}}
        {{~else~}}
            {{~ kankaLocalize 'journal' label ~}}
        {{~/if~}}
    </dt>
    <dd class="path">
    {{~#each ancestors as | ancestorId |~}}
        {{~kankaLinkReference (kankaFindReference ancestorId) ~}}
    {{~/each~}}
    </dd>
{{~/if~}}